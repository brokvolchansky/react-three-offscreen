    "typescript-eslint": "catalog:",
    "zustand": "catalog:"
  },
  "homepage": "https://github.com/pmndrs/react-three-offscreen#readme",
  "dependencies": {
    "mitt": "3.0.1"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.0.0",
    "react": ">=18.0",
    "react-dom": ">=18.0",
    "three": ">=0.133"
  }
}