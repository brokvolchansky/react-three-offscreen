{
  "name": "@react-three/offscreen",
  "version": "0.0.8",
  "description": "Worker offscreen canvas for react three fiber",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "cd examples/vite && npx vite",
    "postbuild": "tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build",
    "test": "echo no tests yet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndrs/react-three-offscreen.git"
  },
  "keywords": [
    "offscreen",
    "worker",
    "threejs",
    "react-three-fiber",
    "webgpu",
    "3d"
  ],
  "author": "Paul Henschel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmndrs/react-three-offscreen/issues"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@babel/plugin-transform-modules-commonjs": "catalog:",
    "@babel/plugin-transform-parameters": "catalog:",
    "@babel/plugin-transform-runtime": "catalog:",
    "@babel/plugin-transform-template-literals": "catalog:",
    "@babel/preset-env": "catalog:",
    "@babel/preset-react": "catalog:",
    "@babel/preset-typescript": "catalog:",
    "@eslint/js": "catalog:",
    "@react-three/drei": "catalog:",
    "@react-three/fiber": "catalog:",
    "@rollup/plugin-babel": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-terser": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-test-renderer": "catalog:",
    "@types/three": "catalog:",
    "babel-loader": "catalog:",
    "babel-plugin-dynamic-import-node": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-react-refresh": "catalog:",
    "globals": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
